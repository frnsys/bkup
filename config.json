{
    "backup": {
        "pony.local": {
            "user": "ftseng",
            "mappings": {
                "~/Desktop/work": "~/Desktop/repos",
                "~/Desktop/projects": "~/Desktop/repos",
                "~/Desktop/STUFF": "~/Desktop/repos",
                "~/Desktop/graphics": "~/Desktop/repos",
                "~/notes": "~/Desktop/repos"
            },
            "externals": {
                "~/Desktop/repos": ["pony/francis", "Dump/francis", "outerspace/francis"]
            }
        },
        "raspberrypi.local": {
            "user": "ftseng",
            "mappings": {
                "~/Desktop/work": "/mnt/crust/francis/repos",
                "~/Desktop/projects": "/mnt/crust/francis/repos",
                "~/Desktop/STUFF": "/mnt/crust/francis/repos",
                "~/Desktop/graphics": "/mnt/crust/francis/repos",
                "~/notes": "/mnt/crust/francis/repos"
