                "~/notes": "/mnt/crust/francis/repos",
                "~/data": "/mnt/crust/francis/repos"
            }
        },
        "localhost": {
            "mappings": {
                "~/work": [
                    "/Volumes/Dump/francis/repos",
                    "/Volumes/pony/francis/repos"
                ],
                "~/projects": [
                    "/Volumes/Dump/francis/repos",
                    "/Volumes/pony/francis/repos"
                ],
                "~/STUFF": [
                    "/Volumes/Dump/francis/repos",
                    "/Volumes/pony/francis/repos"
                ],
                "~/graphics": [
                    "/Volumes/Dump/francis/repos",
                    "/Volumes/pony/francis/repos"
                ],
                "~/notes": [
                    "/Volumes/Dump/francis/repos",
                    "/Volumes/pony/francis/repos"
                ],
                "~/data": [
                    "/Volumes/Dump/francis/repos",
                    "/Volumes/pony/francis/repos"
                ]
            }
        }
    },
    "archive": {
        "svalbard": {
            "user": "ftseng",
            "mappings": {
                "~/archives/work": "/mnt/databank/francis/archives",
                "~/archives/projects": "/mnt/databank/francis/archives",
                "~/archives/STUFF": "/mnt/databank/francis/archives",
