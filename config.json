            "externals": {
                "/mnt/databank/francis/archives": ["/mnt/outerspace/francis"],
                "/mnt/databank/francis/media": ["/mnt/outerspace/francis"]
            },
            "never_delete": true
        },
        "raspberrypi": {
            "user": "ftseng",
            "mappings": {
                "~/archives/work": "/mnt/crust/francis/archives",
                "~/archives/projects": "/mnt/crust/francis/archives",
                "~/archives/STUFF": "/mnt/crust/francis/archives",
                "~/archives/graphics": "/mnt/crust/francis/archives",
                "~/archives/media": "/mnt/crust/francis/media"
            },
            "never_delete": true
        }
    },
    "snapshot": {
        "svalbard": {
            "user": "ftseng",
            "mappings": {
                "~/archives/work": "/mnt/databank/francis/archives",
                "~/archives/projects": "/mnt/databank/francis/archives",
                "~/archives/STUFF": "/mnt/databank/francis/archives",
                "~/archives/graphics": "/mnt/databank/francis/archives",
                "~/archives/media": "/mnt/databank",
                "~/work": "/mnt/databank/francis/repos",
                "~/projects": "/mnt/databank/francis/repos",
                "~/STUFF": "/mnt/databank/francis/repos",
                "~/graphics": "/mnt/databank/francis/repos",
                "~/notes": "/mnt/databank/francis/repos",
                "~/data": "/mnt/databank/francis/repos",
                "~/Downloads": "/mnt/databank",
