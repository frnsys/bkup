                "~/Desktop/tv": ["pony/francis", "Dump/francis", "outerspace/francis"]
            },
            "never_delete": true
        },
        "raspberrypi.local": {
            "user": "ftseng",
            "mappings": {
                "~/Desktop/archives/work": "/mnt/crust/francis/archives",
                "~/Desktop/archives/projects": "/mnt/crust/francis/archives",
                "~/Desktop/archives/STUFF": "/mnt/crust/francis/archives",
                "~/Desktop/archives/graphics": "/mnt/crust/francis/archives",
                "~/Desktop/archives/tv": "/mnt/crust/francis/tv"
            },
            "never_delete": true
        },
    },
    "snapshot": {
        "pony.local": {
            "user": "ftseng",
            "mappings": {
                "~/Desktop/archives/work": "~/Desktop/archives",
                "~/Desktop/archives/projects": "~/Desktop/archives",
                "~/Desktop/archives/STUFF": "~/Desktop/archives",
                "~/Desktop/archives/graphics": "~/Desktop/archives",
