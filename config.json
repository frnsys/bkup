{
    "backup": {
        "svalbard": {
            "user": "ftseng",
            "mappings": {
                "~/work": "/mnt/databank/repos",
                "~/projects": "/mnt/databank/repos",
                "~/STUFF": "/mnt/databank/repos",
                "~/graphics": "/mnt/databank/repos",
                "~/notes": "/mnt/databank/repos",
                "~/data": "/mnt/databank/repos"
            },
            "externals": {
                "/mnt/databank/repos": ["pony/francis", "Dump/francis", "outerspace/francis"]
            }
        },
        "raspberrypi": {
            "user": "ftseng",
            "mappings": {
                "~/work": "/mnt/crust/francis/repos",
                "~/projects": "/mnt/crust/francis/repos",
                "~/STUFF": "/mnt/crust/francis/repos",
                "~/graphics": "/mnt/crust/francis/repos",
                "~/notes": "/mnt/crust/francis/repos",
                "~/data": "/mnt/crust/francis/repos"
            }
        }
    },
    "archive": {
        "svalbard": {
            "user": "ftseng",
            "mappings": {
                "~/archives/work": "/mnt/databank/archives",
                "~/archives/projects": "/mnt/databank/archives",
                "~/archives/STUFF": "/mnt/databank/archives",
                "~/archives/graphics": "/mnt/databank/archives",
                "~/archives/media": "/mnt/databank/media"
            },
            "externals": {
                "~/Desktop/archives": ["pony/francis", "Dump/francis", "outerspace/francis"],
                "~/Desktop/media": ["pony/francis", "Dump/francis", "outerspace/francis"]
            },
            "never_delete": true
        },
        "raspberrypi": {
            "user": "ftseng",
            "mappings": {
                "~/archives/work": "/mnt/crust/francis/archives",
                "~/archives/projects": "/mnt/crust/francis/archives",
                "~/archives/STUFF": "/mnt/crust/francis/archives",
                "~/archives/graphics": "/mnt/crust/francis/archives",
                "~/archives/media": "/mnt/crust/francis/media"
            },
            "never_delete": true
        }
    },
    "snapshot": {
        "svalbard": {
            "user": "ftseng",
            "mappings": {
                "~/archives/work": "/mnt/databank/archives",
                "~/archives/projects": "/mnt/databank/archives",
                "~/archives/STUFF": "/mnt/databank/archives",
                "~/archives/graphics": "/mnt/databank/archives",
                "~/archives/media": "/mnt/databank",
                "~/work": "/mnt/databank/repos",
                "~/projects": "/mnt/databank/repos",
                "~/STUFF": "/mnt/databank/repos",
                "~/graphics": "/mnt/databank/repos",
                "~/notes": "/mnt/databank/repos",
                "~/data": "/mnt/databank/repos",
                "~/Downloads": "/mnt/databank",
                "/tmp/backitup_snapshot": "/mnt/databank"
            },
            "externals": {
                "/mnt/databank/archives": ["pony/francis", "Dump/francis", "outerspace/francis"],
                "/mnt/databank/media": ["pony/francis", "Dump/francis", "outerspace/francis"],
                "/mnt/databank/repos": ["pony/francis", "Dump/francis", "outerspace/francis"]
            },
            "never_delete": true
        },
        "subtasks": {
            "snapshot": []
        }
    }
}
