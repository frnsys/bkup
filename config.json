                "~/Desktop/work": "~/Desktop/repos",
                "~/Desktop/projects": "~/Desktop/repos",
                "~/Desktop/STUFF": "~/Desktop/repos",
                "~/Desktop/graphics": "~/Desktop/repos",
                "~/notes": "~/Desktop/repos",
                "~/Downloads": "~/Desktop",
                "/tmp/backitup_snapshot": "~/Desktop"
            },
            "externals": {
                "~/Desktop/archives": ["pony/francis", "Dump/francis", "outerspace/francis"],
                "~/Desktop/tv": ["pony/francis", "Dump/francis", "outerspace/francis"],
                "~/Desktop/repos": ["pony/francis", "Dump/francis", "outerspace/francis"]
